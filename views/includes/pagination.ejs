<section class="pagination">
    <% if (currentPage !== 1 && previousPage !== 1) { %>
        <a draggable="false" href="?page=1">1</a>
    <% } %>
    <% if (hasPreviousPage) { %>
        <a draggable="false" href="?page=<%= previousPage %>"><%= previousPage %></a>
    <% } %>
    <a href="?page=<%= currentPage %>" class="active"><%= currentPage %></a>
    <% if (hasNextPage) { %>
        <a draggable="false" href="?page=<%= nextPage %>"><%= nextPage %></a>
    <% } %>
    <% if (lastPage !== currentPage && nextPage !== lastPage) { %>
        <a draggable="false" href="?page=<%= lastPage %>"><%= lastPage %></a>
    <% } %>
</section>